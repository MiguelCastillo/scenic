{
  "name": "scenic",
  "version": "0.0.0",
  "description": "Scene graph rendering",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "serve": "npm run scene & npm run editor & npm run fileformats & 3dub",
    "start": "3dub --config .3dub-prod.js",
    "editor": "pakit --config editor/.pakit-editor.js",
    "scene": "pakit --config editor/.pakit-scene.js",
    "fileformats": "pakit --config editor/.pakit-fileformats.js",
    "build": "npm run scene -- --watch false & npm run editor -- --watch false & npm run fileformats -- --watch false"
  },
  "author": "Miguel Castillo",
  "license": "MIT",
  "engines": {
    "node": ">=14.x <=17.x"
  },
  "dependencies": {
    "3dub": "^1.1.0",
    "encoding": "^0.1.13",
    "iconoir-react": "^2.1.0",
    "node-fetch": "^2.6.1",
    "pako": "^2.0.4",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@bit/loader-text": "^1.2.1",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "pakit": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "webgl-mock": "^0.1.7"
  }
}
